// Custom styles.

::-webkit-scrollbar{width:0px}


// post setting
.posts-expand .post-body h2, .posts-expand .post-body h3, .posts-expand .post-body h4, .posts-expand .post-body h5, .posts-expand .post-body h6{
    margin-left: -20px;
}

.footer-inner{
    text-align:center;
}

.header-inner{
	position:static;
    padding : 0;
}

caption, th, td{
    padding : 2px 8px;
    font-size : 12px;
}

table{
    border:1px solid $bg_green;
}
th{
    background-color : $bg_green;
    color:#fff;
}
th, td{
    border-right:1px solid $bg_green;
    border-bottom:1px solid $bg_green;
}

.posts-expand .post-body ul li{
	list-style:disc;
}

blockquote{
    border-left : 4px solid $bg_green;
}

.post-time,.post-title,.posts-expand .post-title-link, .archive-year{
	color: $bg_green;
}

strong{
	text-decoration : underline;
}

#logo{
    position: fixed;
    left: 100px;
    top : 100px;
    width: 100px;
    transform:rotate(10deg);
    img { 
        border-radius:10px; 
    }
}

#top_svg{
    min-height : 274px;
    img {
        margin-left : 100px;
    }
}

.posts-expand .post{
    margin-top:50px;
}

@media screen and (max-width: 700px) {
   #logo,#top_svg{
       display : none;
   } 
}

/*导航栏*/
@media screen and (min-width: 700px) {

    // sidebar
    .sidebar{
        width : 200px; 
        background : whitesmoke;
        box-shadow : none;
        top : 100px;
    }
    .sidebar a{
        color : #555;
    }
    .post-toc .nav .active-current > a{
        color : $bg_green;
        border-bottom-color : $bg_green;
    }
    .post-toc ol a:hover{
        color : $bg_green;
        border-bottom-color : $bg_green;
    }
    .sidebar-nav .sidebar-nav-active {
       color : $bg_green; 
       border-bottom-color : $bg_green;
    }

    // menu
    .menu{
        position: fixed;
        top : 220px
        left :100px;
        text-align : left;
    }

    .main-inner{
        position: relative;
        width:700px;
        left : 50px;
    }

    .menu .menu-item{
        display:block;
    }

    .menu .menu-item a{
        font-size : 1.2rem;
        font-weight : 700;
        color: #EE2E24;
        text-shadow : 1px 1px 0 #FFFFFF;
    }

    /*整体背景图，背景色*/
	.container{
         overflow: hidden; // added for pseudo-element
         position: relative; // added for pseudo-element

         // 解决背景fixed卡顿
         &::before {
            content: ' ';
            position: fixed; // instead of background-attachment
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            background:transparent url("data:image/jpeg;base64,") repeat-y ;
            will-change: transform; // creates a new paint layer
            z-index: -1;
          }
	}
}

html{
    -webkit-overflow-scrolling:touch;
    -webkit-text-size-adjust:100%;
     position: relative; // added for pseudo-element

     &::before {
        content: ' ';
        position: fixed; // instead of background-attachment
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;
        background-image: url("data:image/jpeg;base64,");
        will-change: transform; // creates a new paint layer
        z-index: -1;
    }

}

body,.footer,.header{
    background: transparent;
}

#rewardButton span{
    line-height: 35px;
    border-radius : 50%;
    width:35px;
}

